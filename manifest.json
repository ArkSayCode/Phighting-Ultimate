    "version": "1.0",
    "icons":
    {
        "512": "images/Phighting-Icon.png"
    },
    "author": "ArkSayHi",
    "developer":
    {
        "name": "ArkSayHi"
    },
    "mod":
    {
        "schema_version": 1,
        "payload":
        {
            "wallpaper":
            {
                "dark":
                {
                    "image": "images/DarkageCliffs.png",
                    "text_color": "#ffffff",
